      <text>using RDotNet;
using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using ModelFramework;
using System.ComponentModel;


public class Script 
{      
   [Link()] public Simulation MySimulation;
   public REngine engine = REngine.GetInstance();

   [Input] double day; 
   [Input] double year; 
   public Clock ck;

   // Customizable settings for run (1 and 0) 
   double free = 0;
   double miyoshi = 1;
   double method = 4; // This is either 1-EnKF (w or w/o miyoshi) or 2- GEF (Fully Bayesian) 3- ML, 4- 4DEnVar
   double p = 0.5; 
   double Rcov = 0; // 0 == R is diagonal matrix, independent observation errors
   int[] cropOrder = {1,0,1,1};
   int yrindex = -1; 
   int window_length = 30;
   int window_counter = 0;
   bool Assimilate_day = false;
   int first_obs_day = -1;
   int first_obs_year = -1;
   

   // The following event handler will be called once at the beginning of the simulation
   [EventHandler] public void OnInitialised(){

      engine = Initialize(engine, MySimulation, miyoshi, method, p, Rcov);
      first_obs_day = engine.Evaluate("(strsplit(names(obs.list)[1], '-')[[1]])[1]").AsInteger().Single();
      first_obs_year = engine.Evaluate("(strsplit(names(obs.list)[1], '-')[[1]])[2]").AsInteger().Single();
      
   } // end OnInitialised()

   [EventHandler] public void OnPost(){
         
      // initialize some variables
      engine.Evaluate("specL &lt;- NULL"); 

      // this index allows us to iterate through the correct crop components for each simulation year 
      if (day == 1){
         yrindex += 1; 

         // this section resets the Miyoshi parameters at the beginning of each year (kind of a hack; feel like this should be after a certain number of days without data) 
         //engine.Evaluate("R &lt;- NULL;theta &lt;- NULL"); 
      } 

      // loop through paddocks to collect info from forecast 
      foreach (Paddock paddock in MySimulation.ChildPaddocks) {

         object soil = paddock.LinkByType("SoilWat");
         object soiln = paddock.LinkByType("SoilN");
	 
         ModelFramework.Component crop = paddock.Crops[cropOrder[yrindex]];
         object cropobj = paddock.LinkByType(crop.Name);
         
         // RTM function
         engine = RTM_runner(engine, crop, ((SoilWat) soil), 20);

         // gathering information on this day's forecast
         engine = date_info(engine, day, year, paddock.FullName.ToString());

         // reading forecast from model outputs for the day
         engine = get_model_results(engine, crop, cropobj, ((SoilWat) soil), ((SoilN) soiln));         

      } // end collection for each paddock 

      // data assimilation step (if this is not a free run) 
      if (free == 0){
         // For 4DEnVar if today is greater than the first time we have obs , start counting the window
         if(day &gt;= first_obs_day &amp; year &gt;= first_obs_year &amp; day &lt;330) {
            // Decide if we assimilate today based on the first obs and method
            window_counter = window_counter + 1;
         }
         // step 1: do we have data for this step? 
         NumericMatrix SDA = engine.Evaluate("as.matrix(ifelse(today %in% names(obs.list), 1, 0))").AsNumericMatrix();
         
         if(method != 4) {
            if (SDA[0, 0] == 1)   {
               Assimilate_day = true;
               // if it's sequantial then I need today's info
               engine.Evaluate("Observed &lt;- get_Observed(today, obs.list, dateinfo)");
            }
         }else{
            // if it's 4DEnVar then the first one is enough to get the state vars
            engine.Evaluate("Observed &lt;- obs.list[[1]]");
            
            if(window_counter == window_length) Assimilate_day = true;
         }
         
         // if we have data, let's assimilate!
         if (Assimilate_day){
           
         
               // step 2: prepare the observed data object 
               string[] statVars = engine.Evaluate("Observed$Forecast.name").AsCharacter().ToArray();
               string[] set_modules = engine.Evaluate("Observed$Module").AsCharacter().ToArray();
               string[] set_names = engine.Evaluate("Observed$Apsim.Set.Name").AsCharacter().ToArray();

           
              
               // prepare the forecasted data objects 
               engine.Evaluate("Forecast$X &lt;- get_Forecast(data.all," + day + "," + year + ")"); 
               engine.Evaluate("Maintained &lt;- Forecast$X");
        
            // step 3: data assimilation 
            // `H` is the third Argument of the DA function which provides a mapping between forecasts and observed

            GenericVector DAResult = engine.Evaluate(@"
                             
               if(length(SDA.out)&gt;0) {
               if(!is.null(SDA.out[length(SDA.out)])) {
               tmp.sda.out &lt;- SDA.out %&gt;% purrr::discard(~ length(.x)==1)

               if(length(tmp.sda.out)&gt;0) Forecast$Q &lt;- tmp.sda.out[length(tmp.sda.out)][[1]]$Q
               }
               }
               Forecast$specL &lt;- specL
               vals &lt;- data_assimilation(Forecast, Observed, R, theta, miyoshi, DA.method, p, Rcov)
               DAR &lt;- vals$DAR
               DAR$Time &lt;- Time
               R &lt;- vals$R
               theta &lt;- vals$theta
               adjusted &lt;- nextStep(vals$Xa, Maintained)
               DAR$adjusted&lt;-adjusted
                                       
               SDA.out[[Date_APSIM_to_R(Time$Year, Time$Day)]] = DAR

               adjusted
               ").AsList();

            // step 4: send assimilation ensemble adjustment back to APSIM
            if(DAResult != null){
               write_back(engine, MySimulation, statVars, set_modules, set_names, DAResult);
               window_counter = 0;
               Assimilate_day = false;
            }

         } else{ // if we don't have observations

            engine.Evaluate("DAR &lt;- list()");
            engine.Evaluate("DAR$Time &lt;- Time");
            //engine.Evaluate("DAR$Spectra &lt;- specL"); and removed this line from above: DAR$Spectra &lt;- specL
            engine.Evaluate("SDA.out[[Date_APSIM_to_R(Time$Year, Time$Day)]] = DAR"); 

         }	

      } else{ // if this is a free run 

         engine.Evaluate("DAR &lt;- list()");
         engine.Evaluate("DAR$Time &lt;- Time");
         //engine.Evaluate("DAR$Spectra &lt;- specL");
         engine.Evaluate("SDA.out[[Date_APSIM_to_R(Time$Year, Time$Day)]] = DAR"); 

      } // end free/SDA conditional


      // save CSV file if simulation is over 
      if(ck.is_end_simulation){
         //engine.Evaluate("write.csv(data.all, file = 'dailyOutput.csv', row.names = FALSE)");
         engine.Evaluate("save(SDA.out, file = 'sda.out.Rdata')");      
         engine.Evaluate("save.image('GlobalEnv.RData')");  
               
         //Write model results to the db
         engine.Evaluate("submit_result_Sqlite()");
         engine.Evaluate("data.all &lt;- NULL"); // This variable keeps the model outputs. I empty it after writing it into the db so it would not accumulate over time
         engine.Dispose();
      }

   } // End onPost()     

   // Function to initialize some variables for simulation and storage 
   public REngine Initialize(REngine engine, Simulation MySimulation, double miyoshi, double method, double p, double Rcov){
   
      engine.Evaluate("miyoshi &lt;- " + miyoshi); 
      engine.Evaluate("p &lt;- " + p);
      engine.Evaluate("Rcov &lt;- " + Rcov); 
      engine.Evaluate("DA.method &lt;- " + method);
      engine.Evaluate("window_length &lt;- " + window_length);
      
      ck = (Clock) MySimulation.LinkByName("clock");
      engine.Initialize(); // required since v1.5    
      engine.Evaluate("library(hsdar)");
      engine.Evaluate("library(dplyr)");
      engine.Evaluate("source('EnKF.R')");
   
      // defining variables for storage
      engine.Evaluate("data.all &lt;- NULL;SDA.out &lt;- list();Forecast &lt;- list(Q=1);Observed &lt;- list();Time &lt;- list()");
      engine.Evaluate("X &lt;- NULL;DAR &lt;- NULL;dd &lt;- NULL;R &lt;- NULL;theta &lt;- NULL");  
 
      // get data for the run if this isn't a free run   
      if (free == 0){
         engine.Evaluate("load(list.files(pattern='obs.list'))");
      }
      
      return engine;
   } // end Initialize() function 

   // function to write adjusted values back into the model for the next forecast
   public void write_back(REngine engine, Simulation MySimulation, string[] statVars, string[] set_modules, string[] set_names, GenericVector DAResult){   

      // loop over stat variables that are going to be assimilated
      for (int i = 0; i &lt; statVars.Length; i++) { 
     
         // read the result for the first state variables
         var tmp_res = DAResult[statVars[i]].AsDataFrame();
         engine.SetSymbol("tmp_res", tmp_res);
  
         // now loop over ensembles and write the adjusted variables back to the model     
         for (int j = 0; j &lt; MySimulation.ChildPaddocks.Count; j++) { 

            // subset a row for each ensemble
            double[] row = engine.Evaluate("as.numeric(unlist(tmp_res[" + (j + 1) + ",])) ").AsNumeric().ToArray();

            Paddock paddock_set = MySimulation.ChildPaddocks[j];
                                         
            // set values based on their module
            if (set_modules[i] == "SoilN"){
               object soil_set1 = paddock_set.LinkByType("SoilN");
               ((SoilN) soil_set1).Set(set_names[i], row);		  
                           
            }else if (set_modules[i] == "SoilWat"){
               object soil_set2 = paddock_set.LinkByType("SoilWat");
               ((SoilWat) soil_set2).Set(set_names[i], row);

            }else if (set_modules[i] == "MaizeSoil"){
               object crop = paddock_set.LinkByType("Maize");
               ((Maize) crop).Set(set_names[i], row);

            }                        
         }
      }
   } // end write_back() function
 
   // function to collet model forecast results for the current time step 
   public REngine get_model_results(REngine engine, ModelFramework.Component crop, object cropobj, SoilWat soilwc, SoilN soiln){
      //"WaterSD", "swdef_photo", "transpiration", "swdef_pheno","swdef_expan","sw_supply","sw_demand", "nfact_photo", "nfact_grain"
      // ------- Crop variables
      string[] out_vars = {"biomass","lai","yield"};
      foreach(string out_var in out_vars){
         double tmp_out = 0; 
         crop.Get(out_var, out tmp_out);
         engine.Evaluate("dd$" + out_var + "&lt;-" + Math.Round(tmp_out, 2));
      }

      // ------- Soil state variables
      // Loop through all the layers and collect info on all variables

      for (int i = 0; i &lt; soilwc.dlayer.Length; i++) {

         // ------- soil temp
         //    engine.Evaluate("dd$st"+(i+1)+" &lt;- " + Math.Round(soiln.st[i], 2));

         // ------- soil water
         engine.Evaluate("dd$sw" + (i + 1) + " &lt;- " + Math.Round(soilwc.sw[i], 3));

         // ------- soil no3
         //engine.Evaluate("dd$no3_"+(i+1)+" &lt;- " + Math.Round(soiln.no3[i], 2));
         // ------- soil nh4
         //engine.Evaluate("dd$nh4_"+(i+1)+" &lt;- " + Math.Round(soiln.nh4[i], 2));
         // ------- total soil N
         //engine.Evaluate("dd$ntot_"+(i+1)+" &lt;- " + Math.Round(soiln.nit_tot[i], 2));
         // ------- N mineralization rates 
         //engine.Evaluate("dd$nmin_"+(i+1)+" &lt;- " + Math.Round(soiln.dlt_n_min_tot[i], 2));
         // ------- soil saturated conductivity
          engine.Evaluate("dd$sat" + (i + 1) + " &lt;- " + Math.Round(soilwc.sat[i], 2));
          engine.Evaluate("dd$dul" + (i + 1) + " &lt;- " + Math.Round(soilwc.dul[i], 2));
         engine.Evaluate("dd$ll15" + (i + 1) + " &lt;- " + Math.Round(soilwc.ll15[i], 2));
         // engine.Evaluate("dd$ks" + (i + 1) + " &lt;- " + Math.Round(soilwc.ks[i], 2));
         // ------- soil bulk density
          engine.Evaluate("dd$bd" + (i + 1) + " &lt;- " + Math.Round(soilwc.bd[i], 2));
         // ------- soil saturated water flow conductivity
         engine.Evaluate("dd$swcon" + (i + 1) + " &lt;- " + Math.Round(soilwc.swcon[i], 2));
         int n;

      }
            
      engine.Evaluate("dd$drain &lt;- " + soilwc.drain);
      
      // adding info to simulation storage variables
      engine.Evaluate("dd &lt;- as.data.frame(dd)");
      engine.Evaluate("data.all &lt;- rbind(data.all, dd)");
      
      return engine;
   }  // end get_model_results() function 

   // Function that couples APSIM with RTM model 
   public REngine RTM_runner(REngine engine, ModelFramework.Component crop, SoilWat soilwc, int n){
      
      // initialize storage variables 
      double lai_tmp;
      double biomass;
      double yieldc;
      double LeafGreenNConc = 0;
      double drain = 0; 
      double LeafGreenWt = 0; 
      double GPLA = 0; 
      double biomass_wt = 0; 

      // extract variables for RTM model 
      crop.Get("lai", out lai_tmp);
      crop.Get("yield", out yieldc); 
      crop.Get("biomass", out biomass);
      crop.Get("LeafGreenNConc", out LeafGreenNConc); 
      crop.Get("LeafGreenWt", out LeafGreenWt);
      crop.Get("TPLA", out GPLA);
      crop.Get("biomass_wt", out biomass_wt);

      // PROSAIL function will break if sla is Inf (aka if LeafGreenWt is 0)
      if (LeafGreenWt != 0 &amp; lai_tmp &gt; 0.2){

         // estimating values for RTM model 
         engine.Evaluate("sla &lt;-" + lai_tmp * 10 / (LeafGreenWt));
         
         engine.Evaluate("lai_tmp &lt;-" + lai_tmp);
         engine.Evaluate("psoil &lt;-" + soilwc.sw[0] / soilwc.sat[0]);

         // https://www.sciencedirect.com/science/article/pii/S0303243413000469
         engine.Evaluate("cab &lt;-" + ((LeafGreenNConc * LeafGreenWt) - 0.2057) / 10); 
         engine.Evaluate("car &lt;- cab/4.11");
         engine.Evaluate(@"n &lt;- " + n +@"
            parameter &lt;- data.frame(N =(0.025 + (0.9*sla))/(sla-0.0999999),
            LAI = " + lai_tmp + @",
            Cab=cab,
            Car=car,
            Cw=runif(n,0.01,0.03),
            Cm=(1/sla/1000),
            Cbrown=runif(n,0.01,0.99),
            hspot=runif(n,0.01,.2), 
            psoil=psoil
            )");
         
         engine.Evaluate("spectra &lt;- hsdar::PROSAIL(parameterList = parameter)");  
         engine.Evaluate("temp &lt;- data.frame(vegindex(spectra, index=c('NDVI','OSAVI','EVI','Green NDVI', 'MSAVI','NDVI3')))");
         engine.Evaluate(@"
            dd &lt;- apply(temp, 2, mean, na.rm = TRUE)
            parameter &lt;- data.frame(LeafGreenNConc=" + LeafGreenNConc + ",LeafGreenWt=" + LeafGreenWt + ",lai=" + lai_tmp + ", swc=" + soilwc.sw[0] + ", sat=" + soilwc.sat[0] + "); ");
      } else{
         
         // here we have to add empty values so the row is the correct length 
         engine.Evaluate("spectra &lt;- NULL; parameter &lt;- data.frame()");
         engine.Evaluate(@"dd &lt;- list(NDVI = 0, OSAVI = 0, EVI = 0, `Green NDVI`= 0, MSAVI = 0, NDVI3 = 0)");
      }  
      
      engine.Evaluate("specL &lt;- c(specL, list(parameter)) ");
      return engine;
   } // end RTM_runner() function

   // Function to get information for this time step 
   public REngine date_info(REngine engine, double day, double year, string name){
    
      NumericVector dateinfo = engine.CreateNumericVector(new double[] {day, year}); 
      engine.SetSymbol("dateinfo", dateinfo);
      engine.Evaluate("dd$day &lt;-" + day);
      engine.Evaluate("dd$year &lt;-" + year);
      engine.Evaluate("dd$sim &lt;- getwd()");
      engine.Evaluate("dd$ensemble &lt;- '" + name + "'");
      
      engine.Evaluate("Time$Day &lt;- " + day);
      engine.Evaluate("Time$Year &lt;-" + year); 
      engine.Evaluate("today &lt;- paste0(" + day + ",'-'," + year + ")"); 
      
      return engine;
   } // end data_info() function
         
}

